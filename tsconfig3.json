{
  "compilerOptions": {
    "outDir": "./lib/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "commonjs",
    "target": "es5",
    "stripInternal":true,
    "alwaysStrict":true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "react"
  },
  "include": [
    "./examples/demo/*"
  ]
}
